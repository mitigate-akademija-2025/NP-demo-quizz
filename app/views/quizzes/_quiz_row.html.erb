
<tr id="<%= dom_id quiz %>">
  <td><%= link_to quiz %></td>
  <td><%= quiz.description %></td>
  <td>
    <div style="display: flex; gap: 10px;">
      <%= button_to 'Edit', edit_quiz_path(quiz), method: :get %>
      <%= button_to 'Destroy', quiz, method: :delete %>
    </div>
  </td>
  <td><%= link_to public_quiz_url(quiz.share_token), public_quiz_url(quiz.share_token), target: "_blank" %></td>
</tr>
