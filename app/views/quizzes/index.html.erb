<p style="color: green"><%= notice %></p>

<div>
  <h1>Quizzes</h1>

  <table id="quizzes">
    <thead>
      <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Description</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @quizzes.each do |quiz| %>
        <%= render 'quiz_row', quiz: quiz %>
      <% end %>
    </tbody>
  </table>


  <%= link_to "New quiz", new_quiz_path %>
</div>


